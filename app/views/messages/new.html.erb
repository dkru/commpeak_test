<%= form_tag '/messages', method: :post, class: 'stylized' do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %>
  <%= label_tag :email %>
  <%= email_field_tag :email %>
  <%= label_tag :subject %>
  <%= text_field_tag :subject %>
  <%= label_tag :contents %>
  <%= text_field_tag :contents %>
  <%= submit_tag %>
<% end %>
